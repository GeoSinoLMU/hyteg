Parameters
{
    numFacesPerSide 4;
    discretization P1;
    numCycles 100;
    cycleType V;
    fmgInnerCycles 0; // 0 == no fmg
    sorRelax 1.0;
    preSmoothingSteps 2;
    postSmoothingSteps 2;
    minLevel 2;
    maxLevel 5; // P1 level, P2 level is automatically reduced by 1
    skipCyclesForAvgConvRate 0;
    L2residualTolerance 1e-15;

    outputVTK false;
    outputTiming false;
    outputTimingJSON true;
    outputSQL true;
}
