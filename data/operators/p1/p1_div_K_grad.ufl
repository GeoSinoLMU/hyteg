# generate code with ffc div_K_grad.ufl
#
# Operator: 
# div(K \grad u)
# where K is a 2x2 tensor and scalar u

element = FiniteElement("Lagrange", triangle, 1)

u = TrialFunction(element)
v = TestFunction(element)

forms = [u.dx(0)*v.dx(0)*dx, (u.dx(0)*v.dx(1)+u.dx(1)*v.dx(0))*dx, u.dx(1)*v.dx(1)*dx]

