Parameters
{
  level_h_coarse 2;
  level_h_fine 4;
  elementType 2; # only P2 supported;
  operatorType 1; # 1=variable, 2=polynomial;

  # parameters for surrogate polynomials;
  interpolationLevel 4;
  minPolyDegree 12;
  maxPolyDegree 12;

  max_outer_iter 45;
  max_cg_iter 1000;
  mg_tolerance 1e-14;
  coarse_tolerance 1e-14;

  alpha 1; # alpha->inf => k->step;
  phi 6; # increase phi to decrease smoothness of u;

  vtkOutput 1;
  show_stencil 0;
  n_el 4; # 1 or 4
}
