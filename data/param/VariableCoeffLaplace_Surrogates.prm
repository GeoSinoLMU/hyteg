Parameters
{
  level_h_coarse 2;
  level_h_fine 5;
  elementType 2; # only P2 supported;
  operatorType 1; # 1=variable, 2=polynomial;

  # parameters for surrogate polynomials;
  interpolationLevel 99;
  minPolyDegree 12;
  maxPolyDegree 12;

  max_outer_iter 45;
  max_cg_iter 1000;
  mg_tolerance 1e-15;
  coarse_tolerance 1e-15;

  discontinuous_k 0; # else: smooth k;
  alpha 1; # alpha->inf => k->step;
  phi 6; # increase phi to decrease smoothness of u;

  vtkOutput 0;
}